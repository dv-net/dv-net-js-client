{
  "name": "@dv.net/js-client",
  "version": "0.1.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "TypeScript SDK for DV Net API",
  "author": "DV Net",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dv-net/dv-net-js-client.git"
  },
  "keywords": [
    "dv-net",
    "api",
    "sdk",
    "typescript",
    "merchant",
    "crypto",
    "blockchain"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --minify --clean --out-dir dist"
  },
  "dependencies": {
    "axios": "^1.12.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
